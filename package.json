{
  "name": "barkbox-xt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:changed": "test --watch",
    "test:all": "yarn run test --watchAll --location=global",
    "test:coverage": "yarn run test --coverage --location=global",
    "test:ci": "test --ci",
    "prettier": "prettier --write .",
    "prepare": "husky install --location=global"
  },
  "dependencies": {
    "next": "11.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "cross-env": "^7.0.3",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "eslint-plugin-testing-library": "^5.6.4",
    "husky": "^8.0.1",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "prettier": "^2.7.1"
  }
}
